<GameScreen>:
    BoxLayout:
        orientation: "vertical"
        Image:
            source: 'images/sky.jpg'
            allow_stretch: True
            keep_ratio: False
            size: root.size
        Button:
            text: 'Back to Main Menu'
            pos_hint: {'center_x': 0.5}
            size_hint: (0.2, 0.1)
            on_press: root.manager.current = 'menu'